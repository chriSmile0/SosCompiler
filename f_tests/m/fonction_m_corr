        .data
glob1:  .word   0
glob2:  .word   0
        .text
__start:
li $t1, 1
sw $t1, glob1
li $t1, 2
sw $t1, glob2
jal fonction
lw $a0, glob1
li $v0 1
syscall
lw $a0, glob2
li $v0 1
syscall
li $v0 10
syscall

fonction:
li $t0, 1
beq $t0, $zero, Else0
li $t1, 2
sw $t1, glob1
li $t1, 3
sw $t1, glob2
Else0:
li $v0, 0
jr $ra